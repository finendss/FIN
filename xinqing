local noclipConnection
Tab:AddToggle({
  Callback = function(Value)
    if noclipConnection then
      noclipConnection:Disconnect()
    end
    if Value then -- 开启穿墙
      noclipConnection = game:GetService("RunService").Stepped:Connect(function()
        for _, part in ipairs(player.Character:GetDescendants()) do
          if part:IsA("BasePart") then
            part.CanCollide = false
          end
        end
      end)
    end
  end
})

Tab:AddButton({
  Name = "透视",
  Default = false,
  Callback = function(Value)
local HighlightSystem = {
   Enabled = false,
   Settings = {
       FillColor = Color3.fromRGB(175, 25, 255),
       OutlineColor = Color3.fromRGB(255, 255, 255),
       FillTransparency = 0.5,
       TeamColor = false,  -- 是否使用队伍颜色
       IgnoreLocalPlayer = true,  -- 忽略本地玩家
       MaxDistance = 500,  -- 最大显示距离(0=无限)
       UpdateInterval = 1  -- 更新间隔(秒)
   }
}

local function CreateHighlightSystem()
   local CoreGui = game:GetService("CoreGui")
   local Players = game:GetService("Players")
   local RunService = game:GetService("RunService")
   local lp = Players.LocalPlayer
   
   -- 清理旧系统
   if HighlightSystem.Storage then
       HighlightSystem.Storage:Destroy()
   end
   
   -- 创建存储容器
   HighlightSystem.Storage = Instance.new("Folder")
   HighlightSystem.Storage.Parent = CoreGui
   HighlightSystem.Storage.Name = "AdvancedHighlights_"..os.time()
   
   -- 高亮玩家函数
   local function ApplyHighlight(plr)
       if not HighlightSystem.Enabled then return end
       if plr == lp and HighlightSystem.Settings.IgnoreLocalPlayer then return end
       
       local character = plr.Character or plr.CharacterAdded:Wait()
       local humanoid = character:WaitForChild("Humanoid")
       
       -- 创建高光效果
       local highlight = Instance.new("Highlight")
       highlight.Name = plr.Name
       highlight.Adornee = character
       highlight.FillTransparency = HighlightSystem.Settings.FillTransparency
       highlight.OutlineTransparency = 0
       highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
       
       -- 动态颜色设置
       if HighlightSystem.Settings.TeamColor and plr.Team then
           highlight.FillColor = plr.TeamColor.Color
       else
           highlight.FillColor = HighlightSystem.Settings.FillColor
       end
       
       highlight.OutlineColor = HighlightSystem.Settings.OutlineColor
       highlight.Parent = HighlightSystem.Storage
       
       -- 距离检测(可选)
       if HighlightSystem.Settings.MaxDistance > 0 then
           coroutine.wrap(function()
               while HighlightSystem.Enabled and highlight.Parent do
                   local distance = (lp.Character and character and
                       (lp.Character:GetPivot().Position - character:GetPivot().Position).Magnitude)
                   
                   highlight.Enabled = distance and distance <= HighlightSystem.Settings.MaxDistance
                   task.wait(HighlightSystem.Settings.UpdateInterval)
               end
           end)()
       end
       
       -- 角色变化时重新绑定
       plr.CharacterAdded:Connect(function(newChar)
           if highlight.Parent then
               highlight.Adornee = newChar
           end
       end)
       
       return highlight
   end
   
   -- 初始高亮所有玩家
   for _, player in ipairs(Players:GetPlayers()) do
       if player ~= lp or not HighlightSystem.Settings.IgnoreLocalPlayer then
           coroutine.wrap(ApplyHighlight)(player)
       end
   end
   
   -- 新玩家连接
Players.PlayerAdded:Connect(function(player)
       if HighlightSystem.Enabled then
           ApplyHighlight(player)
       end
   end)
   
   -- 玩家离开时清理
   Players.PlayerRemoving:Connect(function(player)
       local highlight = HighlightSystem.Storage:FindFirstChild(player.Name)
       if highlight then
           highlight:Destroy()
       end
   end)
end

-- 创建UI控制
Tab:AddToggle({
   Name = "fin高级透视",
   Default = false,
   Callback = function(Value)
       HighlightSystem.Enabled = Value
       if Value then
           CreateHighlightSystem()
           OrionLib:MakeNotification({
               Name = "fin提示",
               Content = "透视功能已启用",
               Time = 2
           })
       else
           if HighlightSystem.Storage then
               HighlightSystem.Storage:Destroy()
               HighlightSystem.Storage = nil
           end
       end
   end
})

