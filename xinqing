Tab:AddButton({
   Name = "透视设置",
   Callback = function()
       local SettingsWindow = OrionLib:MakeWindow({
           Name = "透视设置",
           HidePremium = false,
           SaveConfig = false
       })
       
       local SettingsTab = SettingsWindow:MakeTab({
           Name = "参数配置",
           Icon = "rbxassetid://4483345998"
       })
       
       SettingsTab:AddToggle({
           Name = "使用队伍颜色",
           Default = HighlightSystem.Settings.TeamColor,
           Callback = function(Value)
               HighlightSystem.Settings.TeamColor = Value
               if HighlightSystem.Enabled then
                   CreateHighlightSystem() -- 重新初始化以应用更改
               end
           end
       })
       
       SettingsTab:AddToggle({
           Name = "忽略自己",
           Default = HighlightSystem.Settings.IgnoreLocalPlayer,
           Callback = function(Value)
               HighlightSystem.Settings.IgnoreLocalPlayer = Value
           end
       })
       
       SettingsTab:AddSlider({
           Name = "最大距离 ("..HighlightSystem.Settings.MaxDistance..")",
           Min = 0,
           Max = 1000,
           Default = HighlightSystem.Settings.MaxDistance,
           Callback = function(Value)
               HighlightSystem.Settings.MaxDistance = Value
           end
       })
   end
})

