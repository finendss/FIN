local Tab = Window:MakeTab({
	Name = "甩飞与传送",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local flingPlayers = {}
local flingConnections = {}
local flingActive = false

-- 获取附近玩家列表
local function getNearbyPlayers()
    local players = {}
    local localPlayer = game.Players.LocalPlayer
    local localPosition = localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") and localPlayer.Character.HumanoidRootPart.Position
    
    if localPosition then
        for _, player in ipairs(game.Players:GetPlayers()) do
            if player ~= localPlayer and player.Character then
                local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
                if humanoidRootPart then
                    local distance = (humanoidRootPart.Position - localPosition).Magnitude
                    if distance < 100 then -- 100米范围内
                        table.insert(players, player)
                    end
                end
            end
        end
    end
    
    return players
end

-- 更新玩家列表显示
local function updatePlayerList()
    local players = getNearbyPlayers()
    playerDropdown:Refresh(players, true)
end

-- 甩飞选中的玩家
local function flingSelectedPlayers()
    if not flingActive or #flingPlayers == 0 then return end
    
    for _, player in ipairs(flingPlayers) do
        if player.Character then
            local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                -- 创建随机方向的力
                local force = Vector3.new(
                    math.random(-1000, 1000),
                    math.random(500, 1500),
                    math.random(-1000, 1000)
                )
                
                -- 应用力
                humanoidRootPart.Velocity = force
                
                -- 可选：添加旋转效果
                humanoidRootPart.RotVelocity = Vector3.new(
                    math.random(-20, 20),
                    math.random(-20, 20),
                    math.random(-20, 20)
                )
            end
        end
    end
end

Tab:AddDropdown({
    Name = "选择要甩飞的玩家",
    Default = "",
    Options = {},
    Multiple = true, -- 允许多选
    Callback = function(selected)
        flingPlayers = {}
        for _, player in ipairs(selected) do
            table.insert(flingPlayers, player)
        end
    end
})

-- 刷新玩家列表按钮
Tab:AddButton({
    Name = "刷新附近玩家列表",
    Callback = function()
        updatePlayerList()
        OrionLib:MakeNotification({
            Name = "玩家列表已刷新",
            Content = "已更新附近玩家列表",
            Time = 2
        })
    end
})

-- 甩飞强度滑块
Tab:AddSlider({
    Name = "甩飞强度",
    Min = 100,
    Max = 2000,
    Default = 1000,
    Color = Color3.fromRGB(255, 0, 0),
    Increment = 50,
    ValueName = "力度",
    Callback = function(Value)
        flingStrength = Value
    end    
})

-- 开始/停止甩飞开关
Tab:AddToggle({
    Name = "开启甩飞",
    Default = false,
    Callback = function(Value)
        flingActive = Value
        
        if flingActive then
            -- 开始甩飞
            flingConnections.flingLoop = game:GetService("RunService").Heartbeat:Connect(function()
                flingSelectedPlayers()
            end)
            
            OrionLib:MakeNotification({
                Name = "甩飞开启",
                Content = "正在甩选中的玩家",
                Time = 2
            })
        else
            -- 停止甩飞
            if flingConnections.flingLoop then
                flingConnections.flingLoop:Disconnect()
                flingConnections.flingLoop = nil
            end
            
            OrionLib:MakeNotification({
                Name = "甩飞停止",
                Content = "甩飞关闭",
                Time = 2
            })
        end
    end
})

-- 甩飞所有玩家按钮
Tab:AddButton({
    Name = "甩飞所有玩家",
    Callback = function()
        flingPlayers = getNearbyPlayers()
        for i = 1, 5 do -- 连续甩飞5次
            flingSelectedPlayers()
            wait(0.2)
        end
        OrionLib:MakeNotification({
            Name = "甩飞完成",
            Content = "已甩飞所有玩家",
            Time = 3
        })
    end
})

updatePlayerList()


local disableTouchConnection
Tab:AddToggle({
    Name = "防甩飞",
    Default = false,
    Callback = function(Value)
        -- 移除旧的事件监听
        if disableTouchConnection then
            disableTouchConnection:Disconnect()
        end
        
        -- 关闭角色模型所有部件的 CanTouch
        local function disableCanTouch(character)
            for _, descendant in ipairs(character:GetDescendants()) do
                if descendant:IsA("BasePart") then
                    descendant.CanTouch = false
                end
            end
        end
        
        if Value then
            -- 处理当前角色
            if player.Character then
                disableCanTouch(player.Character)
            end
            
            -- 监听角色更换
            disableTouchConnection = player.CharacterAdded:Connect(function(character)
                disableCanTouch(character)
            end)
            
            OrionLib:MakeNotification({
                Name = "提示",
                Content = "已关闭防甩飞",
                Time = 2
            })
        else
            OrionLib:MakeNotification({
                Name = "提示",
                Content = "已开启防甩飞",
                Time = 2
            })
        end
    end
})

